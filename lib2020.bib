	Keywords = {Crack propagation,Fracture,J integral,Level set method,MPM,Material point method,Stress intensity factor,Xiong Zhang},
	Mendeley-Tags = {Fracture,MPM,Xiong Zhang},
	Pages = {541--562},
	Publisher = {Elsevier B.V.},
	Title = {{Material point method with enriched shape function for crack problems}},
	Url = {http://dx.doi.org/10.1016/j.cma.2017.05.012},
	Volume = {322},
