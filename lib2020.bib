@article{Flory1961,
	Abstract = {The free energy of an amorphous, cross-linked polymer is postulated to be expressible as F=F†(T, V)+Fe1(T, $\lambda$), where F† symbolizes the liquid-like contribution from interchain interactions, and F e1 is the elastic contribution attributable to the network structure. The former term is considered to depend only on T and V, and not otherwise on the strain specified by the displacement gradient tensor $\lambda$; F e1 is formulated from the theory of rubber elasticity. The general relationship of stress to strain is derived from F; the temperature coefficient of the logarithm of the measured stress at constant pressure p and distortion $\alpha$=|$\lambda$|-1/3$\lambda$ is shown to differ from that at constant V and $\alpha$ by the linear thermal expansion coefficient of the undistorted polymer; this result holds for any type of strain, provided merely that one (at least) of the normal stresses equates to the constant external pressure. General thermodynamic equations relating the several temperature coefficients of the tension f for constant V and L, for constant p and L, and for constant p and $\alpha$ (extension ratio for fixed volume V) are presented. Expressed as ∂ 1n(f/T)/∂T for the several choices of independent variables indicated, these equations involve the bulk compressibility, the bulk thermal expansion coefficient, and either the coefficient of dilation on stretching or the coefficient of anisotropy of compressibility. Equations of state p(V, T, $\alpha$) and f(V, T, $\alpha$) derived using rubber elasticity theory for formulation of Fe1 afford a basis for evaluating these coefficients. Results depart markedly from those inferred from the theory of infinitesimal deformations. A general basis for interpretation of thermoelastic measurements on high elastic materials is provided. Errors arising from use of previous methods are cited.},
	Author = {Flory, P. J.},
	Doi = {10.1039/TF9615700829},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Flory/1961{\_}Flory.pdf:pdf},
	Issn = {00147672},
	Journal = {Transactions of the Faraday Society},
	Pages = {829--838},
	Title = {{Thermodynamic relations for high elastic materials}},
	Volume = {57},
	Year = {1961},
	Bdsk-Url-1 = {https://doi.org/10.1039/TF9615700829}}

