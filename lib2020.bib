	Year = {2009},
	Bdsk-Url-1 = {http://vbn.aau.dk/files/56400659/Material%7B%5C_%7DPoint%7B%5C_%7DAnalysis%7B%5C_%7Dof%7B%5C_%7DLarge%7B%5C_%7DStrain%7B%5C_%7DProblems.pdf}}

