	Doi = {10.1063/1.2087687},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Lajeunesse, Monnier, Homsy/2005{\_}Lajeunesse, Monnier, Homsy.pdf:pdf},
	Isbn = {1070-6631},
	Issn = {10706631},
	Journal = {Physics of Fluids},
	Keywords = {Channel flow,Friction,Glass,Granular flow,Granular materials,Pipe flow,Shear flow},
	Number = {10},
	Pages = {1--16},
	Pmid = {28479691},
	Title = {{Granular slumping on a horizontal surface}},
