	Issn = {02677261},
	Journal = {Soil Dynamics and Earthquake Engineering},
	Keywords = {Cyclic loading,Cyclic triaxial,Liquefaction,Post-liquefaction,Sand,Sands,Triaxial,Undrained monotonic behaviour},
