	Journal = {Computer Methods in Applied Mechanics and Engineering},
	Keywords = {Boundary elements,Plasticity,Saturated porous media},
	Number = {51-52},
	Pages = {4626--4644},
	Publisher = {Elsevier B.V.},
	Title = {{Consolidation of elastic-plastic saturated porous media by the boundary element method}},
