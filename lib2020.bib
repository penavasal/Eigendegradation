	Journal = {Computational Particle Mechanics},
	Keywords = {Finite deformation,Large deformation,Meshfree,Mixed formulations,Particle finite element method (PFEM),Soil mechanics},
	Mendeley-Tags = {Large deformation,Meshfree},
	Number = {3},
