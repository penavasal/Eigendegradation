	Keywords = {Large deformation,MPM},
	Mendeley-Tags = {Large deformation,MPM},
	Pages = {1--52},
	Title = {{The material point method for simulating continuum materials}},
	Year = {2016},
	Bdsk-Url-1 = {https://doi.org/10.1145/2897826.2927348}}

