	Issn = {00295981},
	Journal = {International Journal for Numerical Methods in Engineering},
	Keywords = {Essential boundary conditions,Meshless Galerkin methods,Natural element method,Natural neighbour co-ordinates,Non-Sibsonian interpolation},
	Number = {1},
	Pages = {1--27},
	Title = {{Natural neighbour Galerkin methods}},
