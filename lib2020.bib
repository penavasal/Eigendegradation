@article{Deshpande2011,
	Abstract = {The objective of the article is to present a unified model for the$\backslash$ndynamic mechanical response of ceramics under compressive stress$\backslash$nstates. The model incorporates three principal deformation mechanisms:$\backslash$n(i) lattice plasticity due to dislocation glide or twinning; (ii)$\backslash$nmicrocrack extension; and (iii) granular flow of densely packed comminuted$\backslash$nparticles. In addition to analytical descriptions of each mechanism,$\backslash$nprescriptions are provided for their implementation into a finite$\backslash$nelement code as well as schemes for mechanism transitions. The utility$\backslash$nof the code in addressing issues pertaining to deep penetration is$\backslash$ndemonstrated through a series of calculations of dynamic cavity expansion$\backslash$nin an infinite medium. The results reveal two limiting behavioral$\backslash$nregimes, dictated largely by the ratio of the cavity pressure p to$\backslash$nthe material yield strength rY. At low values of p/rY, cavity expansion$\backslash$noccurs by lattice plasticity and hence its rate diminishes with increasing$\backslash$nrY. In contrast, at high values, expansion occurs by microcracking$\backslash$nfollowed by granular plasticity and is therefore independent of rY.$\backslash$nIn the intermediate regime, the cavity expansion rate is governed$\backslash$nby the interplay between microcracking and lattice plasticity. That$\backslash$nis, when lattice plasticity is activated ahead of the expanding cavity,$\backslash$nthe stress triaxiality decreases (toward more negative values) which,$\backslash$nin turn, reduces the propensity for microcracking and the rate of$\backslash$ngranular flow. The implications for penetration resistance to high-velocity$\backslash$nprojectiles are discussed. Finally, the constitutive model is used$\backslash$nto simulate the quasi-static and dynamic indentation response of$\backslash$na typical engineering ceramic (alumina) and the results compared$\backslash$nto experimental measurements. Some of the pertinent observations$\backslash$nare shown to be captured by the present model whereas others require$\backslash$nalternative approaches (such as those based on fracture mechanics)$\backslash$nfor complete characterization.},
	Author = {Deshpande, Vikram S. and Gamble, E. A Nell and Compton, Brett G. and McMeeking, Robert M. and Evans, Anthony G. and Zok, Frank W.},
	Doi = {10.1111/j.1551-2916.2011.04516.x},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Deshpande et al/2011{\_}Deshpande et al.pdf:pdf},
	Issn = {00027820},
	Journal = {Journal of the American Ceramic Society},
	Keywords = {Fracture},
	Mendeley-Tags = {Fracture},
	Number = {SUPPL. 1},
	Pages = {204--214},
	Title = {{A constitutive description of the inelastic response of ceramics}},
	Volume = {94},
