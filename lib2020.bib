	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/G{\"{o}}rke et al/2012{\_}G{\"{o}}rke et al.pdf:pdf},
	Issn = {09977538},
	Journal = {European Journal of Mechanics, A/Solids},
	Keywords = {Large strains,Mixed finite element method,Saturated porous media},
	Pages = {88--102},
	Publisher = {Elsevier Masson SAS},
	Title = {{A consistent mixed finite element formulation for hydro-mechanical processes in saturated porous media at large strains based on a generalized material description}},
	Url = {http://dx.doi.org/10.1016/j.euromechsol.2011.09.009},
	Volume = {32},
