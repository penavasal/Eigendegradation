	Number = {26-27},
	Pages = {3293--3323},
	Title = {{Cam-clay plasticity, Part IV: Implicit integration of anisotropic bounding suface model with nonlinear hyperelasticity and ellipsoidal loading function}},
	Volume = {190},
	Year = {2001},
	Bdsk-Url-1 = {https://doi.org/10.1016/S0045-7825(00)00301-7}}

