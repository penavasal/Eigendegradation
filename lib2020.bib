	Issn = {00295981},
	Journal = {International Journal for Numerical Methods in Engineering},
	Keywords = {Cohesive elements,Concrete,Dynamic strength,Fracture,Michael Ortiz,Mixed mode fracture,Size effect,Strain rate effect},
	Mendeley-Tags = {Michael Ortiz},
	Number = {7},
	Pages = {963--994},
	Title = {{Three-dimensional finite-element simulation of the dynamic Brazilian tests on concrete cylinders}},
