	Isbn = {0266352X},
	Issn = {18737633},
	Journal = {Computers and Geotechnics},
	Keywords = {Dynamics,Finite element method,Implicit procedures,Material point method,Quasi-statics,Slope collapse},
	Pages = {159--167},
	Publisher = {Elsevier Ltd},
	Title = {{Development of an implicit material point method for geotechnical applications}},
	Url = {http://dx.doi.org/10.1016/j.compgeo.2015.08.008},
	Volume = {71},
	Year = {2016},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.compgeo.2015.08.008}}

