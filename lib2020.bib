	Keywords = {Dynamics,Interfaces,Stress state,Theory of mixtures,Thermodynamic potential,Unsaturated soil},
	Number = {13 SPEC. ISS.},
	Pages = {1579--1608},
	Title = {{Dynamic behaviour of unsaturated porous media: Governing equations using the Theory of Mixtures with Interfaces (TMI)}},
