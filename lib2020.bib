	Number = {1-2},
	Pages = {219--234},
	Publisher = {Elsevier},
	Title = {{Stabilized low-order finite elements for failure and localization problems in undrained soils and foundations}},
	Volume = {174},
	Year = {1999},
	Bdsk-Url-1 = {https://doi.org/10.1016/S0045-7825(98)00316-8}}

@article{YorkSulsky99,
	Abstract = {The material-point method (MPM) is extended to handle membranes, which$\backslash$nare discretized by a collection of unconnected material points placed$\backslash$nalong each membrane surface. These points provide a Lagrangian description$\backslash$nof the membrane. To solve for the membrane motion, data carried by$\backslash$nthe material points are transferred to a background mesh where equations$\backslash$nof motion are discretized and solved. Then the solution on the background$\backslash$nmesh is used to update the membrane material points. This process$\backslash$nof combining Lagrangian and Eulerian features is standard in MPM;$\backslash$nthe modification for membranes involves merely an implementation$\backslash$nof the constitutive equation in a local, normal-tangential coordinate$\backslash$nsystem. It is shown that this procedure does, in fact, provide adequate$\backslash$nresolution of membranes with thicknesses that can vary substantially$\backslash$nfrom that of the background mesh spacing. A general formulation is$\backslash$ngiven, but the implementa- tion is in a two-dimensional code that$\backslash$nprovides a proof-of-principle. Numerical examples including a spring,$\backslash$npendulum and a string with initial slack are used to illustrate the$\backslash$nmethod. The string with slack uses an additional modification of$\backslash$nthe membrane constitutive equation that allows wrinkles to be modeled$\backslash$nat low computational cost. Presented also are examples of two disks$\backslash$nimpacting, pinching a membrane and rebounding, a difficult problem$\backslash$nfor standard finite element codes. These simulations require a relaxation$\backslash$nof the automatic no-slip contact algorithm in MPM. The addition of$\backslash$nthe capability to model membranes and the new contact algorithm provide$\backslash$na significant improvement over existing methods for handling an important$\backslash$nclass of problems.},
	Author = {York, Allen R. and Sulsky, Deborah and Schreyer, Howard L.},
	Doi = {10.1002/(SICI)1097-0207(19990410)44:10<1429::AID-NME536>3.0.CO;2-4},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/York, Sulsky, Schreyer/1999{\_}York, Sulsky, Schreyer.pdf:pdf},
	Issn = {00295981},
