	Issn = {1612510X},
	Journal = {Landslides},
	Keywords = {Biot theory,Creep,Finite elements,Slow landslides,Viscoplasticity},
	Number = {1},
	Pages = {29--42},
	Title = {{2D viscoplastic finite element modelling of slow landslides: The Portalet case study (Spain)}},
	Volume = {11},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1007/s10346-012-0370-4}}

@article{Sukontasukkul2004,
	Abstract = {In this study, damage mechanics theory was used directly to determine the damage of concrete subjected to both static and impact compressive loadings. Two approaches based on the variations of (1) elastic modulus (E) and (2) strain rate ($\epsilon$̇), were used. Results from both approaches indicated that the damage to concrete at peak load depended mainly on the rate of loading, as it increased with increasing loading rate. The specimens subjected to impact loading were found to suffer higher damage than those subjected to static loading, as seen by the larger value of D at peak load (0.8-0.9 for concrete subjected to impact loading and 0.65 for concrete subjected to static loading). Beyond the peak, the strain energy was sufficient to cause the damage to increase to one (D=1) without any further applied load. {\textcopyright} 2004 Elsevier Ltd. All rights reserved.},
