	Issn = {00295981},
	Journal = {International Journal for Numerical Methods in Engineering},
	Keywords = {1st- And 2nd-order voronoi diagrams,Delaunay triangle,Elastostatics,Natural element method,Natural neighbour interpolation},
	Number = {5},
	Pages = {839--887},
	Pmid = {25246403},
