	Number = {3},
	Pages = {123--132},
	Title = {{Modeling Groundwater Flow under Transient Nonlinear Free Surface}},
	Volume = {8},
	Year = {2003},
	Bdsk-Url-1 = {https://doi.org/10.1061/(asce)1084-0699(2003)8:3(123)}}

@article{Fenton2002,
	Abstract = {Employing the simple iterative technique of adjusting the element positions using computed potentials to locate the free surface can lead to finite elements with large aspect ratios as the free surface drops towards the base of the mesh. In particular, free surface modelling of earth dams with base drains suffer from this problem: The paper suggests a number of steps which can be taken to alleviate mesh distortion problems and improve the numerical stability of the iterative finite element analysis. This leads to a mesh deformation algorithm which adjusts element widths in a simple fashion depending on the free surface height as the iterations proceed. The algorithm is specialized to the sloped earth dam problem, but may find application to other geometries. {\textcopyright} 1997 by John Wiley {\&} Sons, Ltd.},
