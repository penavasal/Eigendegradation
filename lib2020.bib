	Issn = {0266352X},
	Journal = {Computers and Geotechnics},
	Keywords = {Dual-phase,Finite element,Fully-coupled,Porous media,Saturated,Wave propagation},
	Pages = {316--329},
	Publisher = {Elsevier Ltd},
	Title = {{Dual-phase coupled u-U analysis of wave propagation in saturated porous media using a commercial code}},
	Url = {http://dx.doi.org/10.1016/j.compgeo.2013.09.002},
