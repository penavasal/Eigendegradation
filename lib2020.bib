	Journal = {Computers and Mathematics with Applications},
	Keywords = {Differential--algebraic equations,Finite strain viscoelasticity,High-order time-integration,p-version finite element method},
	Number = {4},
	Pages = {441--459},
	Publisher = {Elsevier Ltd},
	Title = {{High-order quasi-static finite element computations in space and time with application to finite strain viscoelasticity}},
