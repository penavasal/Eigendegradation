	Month = {apr},
	Number = {10},
	Pages = {1419--1456},
	Publisher = {John Wiley {\&} Sons, Ltd},
	Title = {{The material point method for simulation of thin membranes}},
	Url = {http://doi.wiley.com/10.1002/{\%}28SICI{\%}291097-0207{\%}2819990410{\%}2944{\%}3A10{\%}3C1429{\%}3A{\%}3AAID-NME536{\%}3E3.0.CO{\%}3B2-4},
