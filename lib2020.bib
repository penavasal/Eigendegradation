@article{Zienkiewicz2013d,
	Abstract = {Standard displacement (irreducible) formulation of elasticity problems fails for problems where the Poisson ratio approaches one-half. Indeed such problems are nearly incompressible which implies that volumetric behavior is negligible compared to deviatoric ones. The application of a mixed formulation for such problems can avoid the difficulties and is of great practical interest as nearly incompressible behavior is encountered in a variety of real engineering problems ranging from soil mechanics to aerospace engineering. Identical problems also arise when the flow of incompressible fluids is encountered. This chapter presents methods that can be applied to both fully incompressible problems (Poisson ratio equal to one-half) as well as nearly incompressible problems. Assessment of various element formulations is first made using the mixed patch test presented in the previous chapter. Later, however, it is shown how other elements also can be treated using so-called stabilize procedures.},
	Author = {Zienkiewicz, Olgierd C. and Taylor, R.L. and Zhu, J.Z.},
	Doi = {10.1016/b978-1-85617-633-0.00010-1},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Zienkiewicz, Taylor, Zhu/2013{\_}Zienkiewicz, Taylor, Zhu(5).pdf:pdf},
	Journal = {The Finite Element Method: its Basis and Fundamentals},
	Pages = {315--359},
	Title = {{Incompressible Problems, Mixed Methods, and Other Procedures of Solution}},
	Volume = {3},
	Year = {2013},
