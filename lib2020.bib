	Keywords = {Michael Ortiz},
	Mendeley-Tags = {Michael Ortiz},
	Number = {3},
	Pages = {539--583},
	Title = {{Fracture paths from front kinetics: Relaxation and rate independence}},
	Volume = {193},
	Year = {2009},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00205-009-0216-y}}

