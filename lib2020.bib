	Issn = {07232632},
	Journal = {Rock Mechanics and Rock Engineering},
	Keywords = {Coupled modelling,Elasto-plastic mohr--Coulomb,Fully 3D FEM ALE,Impulse wave,Landslide--reservoir interaction,Tsunami,Vajont rockslide},
	Number = {6},
	Pages = {2413--2436},
	Publisher = {Springer Vienna},
	Title = {{Landslide spreading, impulse water waves and modelling of the Vajont rockslide}},
