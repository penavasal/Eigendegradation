	Journal = {Engineering Geology},
	Keywords = {Biot's equations,Coupled problems,Free surface,Susana,Unconfined seepage case study},
	Mendeley-Tags = {Susana},
	Pages = {64--73},
	Publisher = {Elsevier B.V.},
	Title = {{Displacement based coupled model for unconfined seepage problems applied to the Gasset Dam (Ciudad Real, Spain)}},
	Url = {http://dx.doi.org/10.1016/j.enggeo.2012.03.014},
