@article{Chiaruttini1996,
	Abstract = {A two-dimensional elastic Chebyshev spectral element method (SPEM) is used to model the seismic wavefield within a massive structure and in its vicinity. We consider 2-D models where a linear elastic structure, with quadrangular cross-section, resting on an elastic homogeneous half-space, is impinged upon by the waves generated by a surface impulse at some distance. The scattering of Rayleigh waves and the response of the structure are extensively analysed in a parametric way, varying size, mechanical parameters and shape of the load. Some of the models considered are representative of embankments and earth dams. The simulation shows that some models resonate, storing part of the incoming energy. With realistic parameters, the lowest resonance frequency is due to pure shear deformation and is controlled by the shear velocity and height of the load. Flexural modes are excited only at higher frequencies. The acceleration at the top of the structure may be five/seven times higher than at the base, depending on the mass of the structure. The gradual release of trapped energy produces a ground roll lasting several seconds after the wave front has passed. The ground-roll amplitude depends on the sturcture's mass and can be as large as 30{\%} of the peak acceleration. Outside resonance conditions, the ground motion is almost unaffected by the presence of the artefact; the horizontal motion on top of it is nearly twice the motion at ground level. Similar results should be expected when the incident field is an upcoming shear wave. A qualitative discussion shows that the presence of anelastic attenuation in the embankment does not significantly alter the preceding conclusions, unless it is of very low values (e.g. Q {\textless} 15). The modelling results that we discuss indicate that the soil-structure interaction may substantially alter the 'free-field' ground motion. From a practical point of view, the main conclusions are: (1) careful analysis is necessary when interpreting seismic records collected in the vicinity of large artefacts; (2) seismic hazard at a site may depend on the presence of man-made structures such as embankments, dams, tall and massive buildings.},
	Author = {Chiaruttini, Claudio and Grimaz, Stefano and Priolo, Enrico},
	Doi = {10.1016/0267-7261(95)00032-1},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Chiaruttini, Grimaz, Priolo/1996{\_}Chiaruttini, Grimaz, Priolo.pdf:pdf},
	Issn = {02677261},
	Journal = {Soil Dynamics and Earthquake Engineering},
	Keywords = {Earth dam,Embankment,Free-field ground motion,Ground motion,Massive structures,Numerical modelling,Rayleigh waves,SPEM,Soil-structure interaction,Spectral element method},
	Number = {2},
	Pages = {75--82},
	Title = {{Modelling of ground motion in the vicinity of massive structures}},
	Volume = {15},
	Year = {1996},
	Bdsk-Url-1 = {https://doi.org/10.1016/0267-7261(95)00032-1}}

