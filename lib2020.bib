@article{Ligett,
	Abstract = {This paper presents a modified Particle- in-Cell (PIC) method for sea ice dynamics modeling. The proposed approach combines Eulerian and Lagrangian methods to improve the efficiency and accuracy of the PIC model. The motion of Lagrangian particles is superimposed by a fixed Eulerian grid. The internal ice force is modeled with a viscous-plastic constitutive law. Background information on sea ice dynamics and the numerical implementation of the modified PIC model are discussed in detail. The introduction of a Gaussian interpolation kernel significantly improves ice edge prediction and continuity, as shown by select test cases. Results compare favorably with the Particle-in-Cell calculations presented by Wang (2000), with minor disadvantages indicating the need for further development of the Gaussian interpolation methods. With further adaptations, this modified PIC method could be applied to large-scale sea ice prediction in polar regions, as well as meso-scale prediction in the Marginal Ice Zone (MIZ).},
	Author = {Ligett, Katrina},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Ligett/Unknown{\_}Ligett.pdf:pdf},
	Keywords = {Sea ice dynamics modeling},
	Pages = {21},
	Title = {{A Modified Particle-in-Cell Method for Sea Ice Dynamics}}}

@article{Conte2019,
	Abstract = {Traditional numerical methods such as the finite element method are usually used to analyse the slope response in the pre-failure and failure stages under the assumption of small deformations. However, these methods are generally unsuitable for simulating the post-failure stage of landslides due to the occurrence of large deformations. The material point method (MPM) is a numerical technique capable to overcome this limitation. In the present study, MPM is used to carry out a two-dimensional analysis of the run-out process of the Senise landslide that occurred in Southern Italy, in 1986. Accuracy of the method is assessed by comparing the final geometry of the landslide observed just after the event and the magnitude of the measured displacements, to those provided by the numerical simulation. The calculation results match fairly well the observed ones when two slip surfaces detected by the inclinometers, are accounted for in the analysis. An additional improvement of simulation is achieved taking into account the presence of some buildings and retaining walls existing in that area. Kinematics of the landslide is also investigated. The results show that the Senise landslide was a translational slide moving along two planar slip surfaces located in thin layers of clayey silt. In addition, these results demonstrate that a suitable analysis of the post-failure stage can lead to a better understanding of the complex mechanical processes that characterise some landslides, and thereby help in establishing the most effective stabilization measures.},
	Author = {Conte, Enrico and Pugliese, Luigi and Troncone, Antonello},
	Doi = {10.1016/j.enggeo.2019.03.006},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Conte, Pugliese, Troncone/2019{\_}Conte, Pugliese, Troncone.pdf:pdf},
	Issn = {00137952},
	Journal = {Engineering Geology},
	Keywords = {Landslide,Landslides,MPM,Material point method,Numerical modelling,Post-failure stage,Run-out simulation},
	Mendeley-Tags = {Landslides,MPM},
	Number = {November 2018},
	Pages = {149--159},
	Publisher = {Elsevier},
	Title = {{Post-failure stage simulation of a landslide using the material point method}},
	Url = {https://doi.org/10.1016/j.enggeo.2019.03.006},
	Volume = {253},
	Year = {2019},
	Bdsk-Url-1 = {https://doi.org/10.1016/j.enggeo.2019.03.006}}

