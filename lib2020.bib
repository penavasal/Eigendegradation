	Journal = {Computer Methods in Applied Mechanics and Engineering},
	Keywords = {Elasto-plasticity,Finite deformation mechanics,Generalised interpolation,Lagrangian mechanics,Large deformation,MPM,Material point method,Plasticity,elasto-plasticity,finite deformation mechanics,generalised interpolation,lagrangian mechanics,material point method},
	Mendeley-Tags = {Large deformation,MPM,Plasticity},
