@article{Sabetamal2014,
	Abstract = {This paper outlines the development as well as implementation of a numerical procedure for coupled finite element analysis of dynamic problems in geomechanics, particularly those involving large deformations and soil-structure interaction. The procedure is based on Biot's theory for the dynamic behaviour of saturated porous media. The nonlinear behaviour of the solid phase of the soil is represented by either the Mohr Coulomb or Modified Cam Clay material model. The interface between soil and structure is modelled by the so-called node-to-segment contact method. The contact algorithm uses a penalty approach to enforce constraints and to prevent rigid body interpenetration. Moreover, the contact algorithm utilises a smooth discretisation of the contact surfaces to decrease numerical oscillations. An Arbitrary Lagrangian-Eulerian (ALE) scheme preserves the quality and topology of the finite element mesh throughout the numerical simulation. The generalised-$\alpha$ method is used to integrate the governing equations of motion in the time domain. Some aspects of the numerical procedure are validated by solving two benchmark problems. Subsequently, dynamic soil behaviour including the development of excess pore-water pressure due to the fast installation of a single pile and the penetration of a free falling torpedo anchor are studied. The numerical results indicate the robustness and applicability of the proposed method. Typical distributions of the predicted excess pore-water pressures generated due to the dynamic penetration of an object into a saturated soil are presented, revealing higher magnitudes of pore pressure at the face of the penetrometer and lower values along the shaft. A smooth discretisation of the contact interface between soil and structure is found to be a crucial factor to avoid severe oscillations in the predicted dynamic response of the soil. {\textcopyright} 2013 Elsevier Ltd.},
	Author = {Sabetamal, Hassan and Nazem, Majidreza and Carter, John P. and Sloan, Scott W.},
	Doi = {10.1016/j.compgeo.2013.08.005},
	File = {:Users/pedro/MEGA/MEGAsync/Biblio{\_}mendeley/Sabetamal et al/2014{\_}Sabetamal et al.pdf:pdf},
	Issn = {0266352X},
	Journal = {Computers and Geotechnics},
	Keywords = {ALE method,Contact mechanics,Dynamic coupled analysis,Full formulation,Large deformation,Pile installation,Porous media,Time integration},
	Mendeley-Tags = {Full formulation,Large deformation},
	Pages = {117--131},
	Publisher = {Elsevier Ltd},
	Title = {{Large deformation dynamic analysis of saturated porous media with applications to penetration problems}},
	Url = {http://dx.doi.org/10.1016/j.compgeo.2013.08.005},
	Volume = {55},
	Year = {2014},
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/j.compgeo.2013.08.005}}

